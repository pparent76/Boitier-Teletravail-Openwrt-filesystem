# Server configuration
config uhttpd main

	# HTTP listen addresses, multiple allowed
	list listen_http	0.0.0.0:8888
	list listen_http	[::]:8888

	# Server document root
	option home		/site/

	# Common name
	option commonname	'LEDE'

